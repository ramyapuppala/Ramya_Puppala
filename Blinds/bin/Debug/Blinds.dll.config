<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<add key="url" value="https://www.blinds.com/"/>

		<add key ="grid" value="Y"/>

		<!-- Links -->
		<add key="PriceLowtoHigh_xpath" value="//a[contains(@href, 'sorts=Price')]"/>
		
		<!-- TextField-->
		<add key="search_id" value="gcc-inline-search"/>

		<!-- List of elements -->
		<add key="PriceLowtoHigh_elements_xpath" value="//div[@class='f4 black fw7']"/>


		<!-- Button -->
		<add key="search_button_id" value="gcc-inline-search-submit"/>
		<add key="search_xpathList" value="//span[@testid='gcc-product-discount-price']"/>
		<add key="See_More_xpath" value=" //*[@id='see-more-products-btn']"/>


		<!-- Email -->
		<add key="email_id" value="email"/>
	</appSettings>
</configuration>